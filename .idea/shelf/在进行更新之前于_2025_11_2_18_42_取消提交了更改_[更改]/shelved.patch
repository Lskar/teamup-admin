     @Override
     public String exportResumeData(String status, Long projectId) {
-        return "";
+        try {
+            // 构建查询条件
+            LambdaQueryWrapper<ResumeDO> queryWrapper = new LambdaQueryWrapper<>();
+
+            // 根据状态筛选简历
+            if (status != null && !status.isEmpty()) {
+                // 根据 resume 表的 status 字段定义（0-待审核 1-已发布 2-已删除）
+                switch (status) {
+                    case "pending":
+                        queryWrapper.eq(ResumeDO::getStatus, 0);
+                        break;
+                    case "published":
+                        queryWrapper.eq(ResumeDO::getStatus, 1);
+                        break;
+                    case "deleted":
+                        queryWrapper.eq(ResumeDO::getStatus, 2);
+                        break;
+                    default:
+                        // 如果传入其他状态值，尝试解析为数字
+                        try {
+                            Integer statusValue = Integer.parseInt(status);
+                            queryWrapper.eq(ResumeDO::getStatus, statusValue);
+                        } catch (NumberFormatException e) {
+                            // 忽略无效的状态值
+                        }
+                        break;
+                }
+            }
+
+            // 如果提供了项目ID，需要关联查询 resume_delivery 表
+            if (projectId != null) {
+                // 使用自定义SQL查询关联数据
+                QueryWrapper<ResumeDO> customQuery = new QueryWrapper<>();
+                customQuery.inSql("id", "SELECT id FROM resume_delivery WHERE project_id = " + projectId);
+
+                if (projectId != null) {
+                    // 使用 LambdaQueryWrapper 并添加子查询条件
+                    queryWrapper.apply("EXISTS (SELECT 1 FROM resume_delivery rd WHERE rd.resume_id = resume.id AND rd.project_id = {0})", projectId);
+                }
+
+
+                queryWrapper = customQuery.lambda();
+            }
+
+            // 查询简历数据
+            List<ResumeDO> resumeList = list(queryWrapper);
+
+            // 如果没有数据，返回空字符串
+            if (resumeList.isEmpty()) {
+                return "";
+            }
+
+            // 生成导出文件名
+            String fileName = "resumes_export_" + System.currentTimeMillis() + ".xlsx";
+            // 使用系统临时目录
+            String tempDir = System.getProperty("java.io.tmpdir");
+            String exportPath = tempDir + java.io.File.separator + fileName;
+
+            // 创建工作簿
+            org.apache.poi.xssf.usermodel.XSSFWorkbook workbook = new org.apache.poi.xssf.usermodel.XSSFWorkbook();
+            org.apache.poi.ss.usermodel.Sheet sheet = workbook.createSheet("简历数据");
+
+            // 创建表头
+            org.apache.poi.ss.usermodel.Row headerRow = sheet.createRow(0);
+            String[] headers = {"ID", "申请人ID", "简历标题", "文件格式", "文件大小(字节)", "创建时间", "状态"};
+            for (int i = 0; i < headers.length; i++) {
+                org.apache.poi.ss.usermodel.Cell cell = headerRow.createCell(i);
+                cell.setCellValue(headers[i]);
+            }
+
+            // 填充数据
+            for (int i = 0; i < resumeList.size(); i++) {
+                ResumeDO resume = resumeList.get(i);
+                org.apache.poi.ss.usermodel.Row row = sheet.createRow(i + 1);
+
+                row.createCell(0).setCellValue(resume.getId() != null ? resume.getId() : 0L);
+                row.createCell(1).setCellValue(resume.getApplicantId() != null ? resume.getApplicantId() : 0L);
+                row.createCell(2).setCellValue(resume.getResumeTitle() != null ? resume.getResumeTitle() : "");
+                row.createCell(3).setCellValue(resume.getFileFormat() != null ? resume.getFileFormat() : "");
+                row.createCell(4).setCellValue(resume.getFileSize() != null ? resume.getFileSize() : 0L);
+
+                // 处理日期字段
+                if (resume.getCreatedTime() != null) {
+                    org.apache.poi.ss.usermodel.Cell dateCell = row.createCell(5);
+                    dateCell.setCellValue((Date) resume.getCreatedTime());
+                }
+
+                // 处理状态字段
+                String statusText = "未知";
+                if (resume.getStatus() != null) {
+                    switch (resume.getStatus()) {
+                        case 0: statusText = "待审核"; break;
+                        case 1: statusText = "已发布"; break;
+                        case 2: statusText = "已删除"; break;
+                        default: statusText = "未知(" + resume.getStatus() + ")";
+                    }
+                }
+                row.createCell(6).setCellValue(statusText);
+            }
+
+            // 自动调整列宽
+            for (int i = 0; i < headers.length; i++) {
+                sheet.autoSizeColumn(i);
+            }
+
+            // 写入文件
+            java.io.FileOutputStream fileOut = new java.io.FileOutputStream(exportPath);
+            workbook.write(fileOut);
+            fileOut.close();
+            workbook.close();
+
+            return exportPath;
+
+        } catch (Exception e) {
+            // 记录错误日志
+            e.printStackTrace();
+            // 发生异常时返回空字符串
+            return "";
+        }
     }
+
 }
